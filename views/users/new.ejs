<form class="form-signup" action="/users" method="POST">
    <label for="name">UserName:</label>
    <input id="q" type="text" name="name" id="name">
    
    <label for="email">Email:</label>
    <input id="q" type="email" name="email" id="email">

    <label for="password">Password:</label>
    <input id="q" type="password" name="password" id="password">

    <button id="q" class="submit-btn" type="submit">Sign Up</button>
</form>